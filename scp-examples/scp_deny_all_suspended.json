{
   "Version": "2012-10-17",
   "Statement": [
      {
         "Sid": "DenyAllAccessExceptOrganization",
         "Effect": "Deny",
         "Action": "*",
         "Resource": [
            "*"
         ],
         "Condition": {
            "StringNotLike": {
               "aws:PrincipalArn": [
                  "arn:aws:iam::*:role/OrganizationAccountAccessRole"
               ]
            }
         }
      }
   ]
}